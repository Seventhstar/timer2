<p id="notice"><%= notice %></p>

<h1>Listing Ut Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ut_types.each do |ut_type| %>
      <tr>
        <td><%= ut_type.name %></td>
        <td><%= link_to 'Show', ut_type %></td>
        <td><%= link_to 'Edit', edit_ut_type_path(ut_type) %></td>
        <td><%= link_to 'Destroy', ut_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ut type', new_ut_type_path %>
